<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body>
    <main class="auth-page-container">
      <div class="card" style="width: 350px">
        <div class="card-body">
          <h2 class="mb-3">Change Password</h2>
          <div class="mb-3">
            <label for="email" class="form-label">Email address</label>
            <input
              type="email"
              class="form-control"
              id="email"
              placeholder="name@example.com"
            />
          </div>
          <div class="mb-3">
            <label for="password" class="form-label">Password</label>
            <input
              type="password"
              class="form-control"
              id="password"
              placeholder="**********"
            />
          </div>
          <div class="mb-3">
            <label for="changepassword" class="form-label">ChangePassword</label>
            <input
              type="password"
              class="form-control"
              id="changepassword"
              placeholder="**********"
            />
          </div>
          <button class="btn btn-primary" id="resetbutton">RESET PASSWORD</button>
        </div>
      </div>
    </main>
    <script src="./createuser.js"></script>
  </body>
</html>